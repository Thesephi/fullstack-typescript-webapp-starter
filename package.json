{
  "name": "fullstack-typescript-webapp-starter",
  "version": "1.1.0",
  "description": "A db-agnostic, React-based web application starter boilerplate",
  "scripts": {
    "build": "scripts/build.sh",
    "start-dev": "WATCH=true NODE_ENV=development scripts/start-dev.sh",
    "start": "NODE_ENV=production node build/server/main.js",
    "test": "scripts/test.sh",
    "dev:build:fe": "webpack --config ./src/client/webpack.config.js",
    "dev:copy:be": "cp -r ./src/server/view-templates ./build/server/",
    "dev:start": "LOG_LEVEL=info PORT=3000 node build/server/main.js"
  },
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/restify": "^8.4.2",
    "css-loader": "^5.0.0",
    "file-loader": "^6.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.3",
    "source-map-loader": "^1.0.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.1.2",
    "ts-loader": "^8.0.1",
    "typescript": "^4.0.0-beta",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "fontsource-roboto": "^3.0.3",
    "react": "0.0.0-experimental-7f28234f8",
    "react-dom": "0.0.0-experimental-7f28234f8",
    "somnus": "^3.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^shared/(.*)$": "<rootDir>/src/shared/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "./src/client/tsconfig.json"
      }
    }
  },
  "license": "MIT",
  "contributors": [
    "Khang Dinh <thesephi@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Thesephi/fullstack-typescript-webapp-starter"
  },
  "keywords": [
    "react",
    "nodejs",
    "webapp",
    "boilerplate"
  ]
}
