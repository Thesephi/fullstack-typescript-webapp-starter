{
  "name": "fullstack-typescript-webapp-starter",
  "version": "1.0.0",
  "description": "A db-agnostic, React-based web application starter boilerplate",
  "scripts": {
    "build": "scripts/build.sh",
    "start": "WATCH=true NODE_ENV=development scripts/start.sh",
    "test": "jest",
    "dev:build:fe": "webpack --config ./src/client/webpack.config.js",
    "dev:copy": "cp -r ./src/server/view-templates ./build/server/",
    "dev:start": "LOG_LEVEL=info PORT=3000 node build/server/main.js"
  },
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/restify": "^8.4.2",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "source-map-loader": "^1.0.1",
    "ts-jest": "^26.1.2",
    "ts-loader": "^8.0.1",
    "typescript": "^4.0.0-beta",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.29.0",
    "@blueprintjs/icons": "^3.19.0",
    "react": "0.0.0-experimental-7f28234f8",
    "react-dom": "0.0.0-experimental-7f28234f8",
    "somnus": "^3.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsConfig": "./src/client/tsconfig.json"
      }
    }
  }
}
